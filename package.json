{
    "name": "cfactor-minigame-pack",
    "version": "1.0.0",
    "description": "A starting template for c-factor mini game",
    "scripts": {
        "start": "webpack-dev-server --config webpack.dev.config.js -d --inline --hot --progress --port 8000 --mode development",
        "build": "rimraf ./dist && webpack --config webpack.prod.config.js -p --progress --mode production",
        "deploy": "webpack --config webpack.dev.config.js -p --progress --mode production",
        "lint": "tslint src/**/*.ts",
        "lint:fix": "npm run lint -- --fix"
    },
    "engines": {
        "node": ">= 7.0.0"
    },
    "author": "Musab Hussain",
    "devDependencies": {
        "copy-webpack-plugin": "^5.0.3",
        "expose-loader": "^0.7.5",
        "html-webpack-plugin": "^3.2.0",
        "rimraf": "^2.6.3",
        "ts-loader": "^5.4.5",
        "tslint": "^5.16.0",
        "tslint-loader": "^3.5.4",
        "tslint-webpack-plugin": "^2.0.4",
        "typescript": "^3.4.5",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1",
        "webpack-dev-server": "^3.3.1",
        "webpack-obfuscator": "^0.18.0"
    },
    "dependencies": {
        "@types/howler": "^2.1.0",
        "howler": "^2.1.2",
        "phaser-ce": "^2.12.0",
        "webpack-strip-block": "^0.2.0"
    }
}